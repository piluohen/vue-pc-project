baseImagesUrl = "//static.unimedia.com/fe/cms/images/"

@keyframes iconRotate
  from
    transform rotate(0deg)
  to
    transform rotate(180deg)

.pagination-bar
  margin-top: 20px

.el-dialog__title
  font-size 16px

.el-dialog__body
  padding 20px

.vertical-top
  vertical-align top

.fn-right
  float right

.fn-left
  float left

.clearfix:after
  content ''
  width 0
  height 0
  display block
  clear both

.font-success
  color #67c23a

.font-gray
  color #999999

.font-blue
  color #409EFF

.font-red
  color: #F56C6C

.font-warn
  color #E6A23C

.el-input--small .el-input__inner
  line-height 30px

/*滚动条整体部分,必须要设置*/
::-webkit-scrollbar
  width: 4px
  height: 4px
  background-color: #f2f2f2

/*滚动条的轨道*/
::-webkit-scrollbar-track
  box-shadow: inset 0 0 2px rgba(0,0,0,.3)
  background-color: #f2f2f2

/*滚动条的滑块按钮*/
::-webkit-scrollbar-thumb
  border-radius: 4px
  background-color: #aaaaaa
  box-shadow: inset 0 0 2px #aaaaaa
